   "source": [
    "class Solution:\n",
    "    def fib(self, n: int) -> int:\n",
    "        if n == 0:\n",
    "            return 0 \n",
    "        dp_i_1, dp_i_0 = 1, 0\n",
    "        for i in range(1, n):\n",
    "            dp_i_1, dp_i_0 = dp_i_1 + dp_i_0, dp_i_1\n",
    "        return dp_i_1 % 1000000007"
   ]
  },
